<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://services.ajourneytoawesome.com/</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>dallaswin200.arvixeshared.com:8172</MSDeployServiceURL>
    <DeployIisAppPath>services.ajourneytoawesome.com</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>services_somecom_pub</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="2" Enabled="False">
          <Destination Path="Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\DefaultConnection_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="Awesome.Entities.AwesomeEntities" Order="3" Enabled="True">
          <Destination Path="Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="Awesome.Entities.AwesomeEntities, Awesome.Entities" MigrationConfiguration="Awesome.Entities.Migrations.Configuration, Awesome.Entities" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Awesome.Entities.AwesomeEntities-Web.config Connection String">
      <ParameterValue>Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=localhost;Initial Catalog=AwesomeDataLive;User ID=Awesome_User2;Password=unknown_1</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>